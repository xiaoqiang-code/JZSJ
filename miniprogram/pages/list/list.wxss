::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.van-swipe-cell__left,
.van-swipe-cell__right {
  border-radius:0px 9px 9px 0px;
  display: inline-block;
  width: 65px;
  height: 42px;
  font-size: 15px;
  line-height: 170px;
  color: #fff;
  text-align: center;
  /* align-items: center; */
  background-color: #03BCD4;

}
