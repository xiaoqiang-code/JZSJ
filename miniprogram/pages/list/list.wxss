/* pages/list/list.wxss */
.myself{
  width:94%;
  height:170px;
  margin-top: 10px;
  margin-left: 3%;
  background-color:white;
  border:1px solid #03BCD4;
  border-radius: 10px;
  box-shadow: 5px 5px 5px 0 rgba(0, 0, 0, .05);
  -webkit-box-shadow: #d4d2d2 0px 0px 10px;
  -moz-box-shadow: #d4d2d2 0px 0px 10px;

}
